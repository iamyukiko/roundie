<div class="container">
  <h4 class="mt-2 p-2 text-center bg-secondary text-white col-md-3">イベント一覧</h4>
  <table class="table table-bordered">
    <thead class="thead-light">
      <tr>
        <th>イベントID</th>
        <th>イベント名</th>
        <th>開催日</th>
        <th>募集締め切り日</th>
        <th>主催者</th>
        <th>開催エリア</th>
        <th>ステータス</th>
      </tr>
    </thead>
    <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= link_to event.event_title, admin_event_path(event.id) %></td>
        <td><%= event.event_date %></td>
        <td><%= event.deadline_date %></td>
        <td><%= event.owner_id %></td>
        <td><%= event.event_area %></td>
        <!--<td>< if user.is_valid == true %>-->
      <!--  <p class="text-success font-weight-bold">有効</p>-->
      <!--  < else %>-->
      <!--  <p class="text-secondary font-weight-bold">退会</p>-->
      <!--< end %>-->
      </tr>
    <% end %>
    </tbody>
  </table>
  <!--<= paginate @customers %>-->
</div>