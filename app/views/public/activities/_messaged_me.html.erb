<% user =  User.find_by(id: activity.subject.room.entries.pluck(:user_id).select{|o| o != current_user.id }) %>
<%= link_to read_activities_path(current_user, activity), class: "dropdown-item border-bottom #{'read' if activity.read?}", method: :patch do %>
  <%= image_tag activity.subject.user.get_image, class: "rounded-circle", size:'50x50' %>
  <%= user.nickname %> からメッセージが届きました
<% end %>