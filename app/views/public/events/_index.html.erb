<table class="table">
  <thead>
    <tr>
      <th>開催日</th>
      <th>イベントタイトル</th>
      <th>開催エリア</th>
      <th>参加状況</th>
      <th>締め切り</th>
    </tr>
  </thead>
  <tbody>
    <% events.each do |event| %>
    <tr>
      <td><%= link_to event.event_date, event_path(event.id) %></td>
      <td><%= event.event_title %></td>
      <td><%= event.event_area %></td>
      <td><%= event.entry_limit %>人</td>
      <td>残り<%= event.date %>日</td>
    </tr>
    <% end %>
  </tbody>
</table>