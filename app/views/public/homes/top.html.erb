<h1>トップページ</h1>
<table>
  <table>
    <thead>
      <tr>
        <td>開催日</td>
        <td>イベントタイトル</td>
        <td>開催エリア</td>
        <td>参加状況</td>
        <td>締め切り</td>
      </tr>
    </thead>
     <% @events.first(5).each do |event| %>
    <tbody>
      <tr>
        <td><%= event.event_date %></td>
        <td><%= link_to event_path(event) do %><%= event.event_title %><% end %></td>
        <td><%= event.event_area %></td>
        <td><%= event.users.count %> / <%= event.entry_limit %>人</td>
        <td>あと<%= event.date %>日</td>
      </tr>
    </tbody>
    <% end %>
  </table>
    <%= link_to "＞＞もっと見る", events_path %>


