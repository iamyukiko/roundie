  <%= image_tag user.get_image, size:'200x200' %></br>
  <p><strong><%= user.user_name %></strong>&nbsp;
     <%= render "public/relationships/relationship_btn", user: user %>&nbsp;
     <%= render "public/rooms/room", isRoom: isRoom, roomId: roomId, room: room, entry: entry, user: user %></p>
  <p><%= link_to user.followings.count, user_followings_path(user) %>フォロー | <%= link_to user.followers.count, user_followers_path(user) %>フォロワー</p>
